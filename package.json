{
  "name": "vue-class-to-options",
  "version": "0.0.6",
  "description": "An utility to convert Vue.js Single File Components from Class API to option API.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "classToOptions": "./dist/index.js"
  },
  "preferGlobal": true,
  "repository": "https://github.com/vertcitron/vue-class-to-options.git",
  "author": "St√©phane Souron <stef@vertcitron.fr>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "parcel build src/index.ts --target node",
    "watch": "parcel watch src/index.ts --target node",
    "start": "yarn build && node bin/classToOptions.js",
    "dev": "ts-node-dev --respawn --clear src/run.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/figlet": "^1.2.0",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "jest": "^25.1.0",
    "parcel-bundler": "^1.12.4",
    "rollup": "^2.0.3",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.2.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "figlet": "^1.3.0",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  }
}
