{
  "name": "vue-class-to-options",
  "version": "0.1.2",
  "description": "An utility to convert Vue.js Single File Components from Class API to option API.",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "classToOptions": "./bin/classToOptions.js"
  },
  "preferGlobal": true,
  "repository": "https://github.com/vertcitron/vue-class-to-options.git",
  "author": "St√©phane Souron <stef@vertcitron.fr>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "watch": "yarn build && tsc --watch",
    "start": "yarn build && node bin/classToOptions.js",
    "dev": "ts-node-dev --respawn --clear src/run.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/figlet": "^1.2.0",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "figlet": "^1.3.0",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  },
  "resolutions": {
    "acorn": "^6.4.1",
    "minimist": "^1.2.3"
  }
}
